{
    "version": 2,
    "builds": [
        {
            "src": "package.json",
            "use": "@vercel/static-build",
            "config": {
                "distDir": "dist"
            }
        },
        {
            "src": "server/vercel.ts",
            "use": "@vercel/node"
        }
    ],
    "rewrites": [
        {
            "source": "/(background|upload-background|remove-background|login|admin-data|uploads/.*)",
            "destination": "/server/vercel.ts"
        }
    ]
}
